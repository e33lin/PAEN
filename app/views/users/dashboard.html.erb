<h1>Welcome <%= @user.first_name.capitalize %>!</h1>

<table>
  <thead>
    <tr>
      <th>My Teams</th>
      <th>Survey Completed</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teams.each do |team| %>
      <tr>
        <td><%= link_to team.name, user_team_list_path(id: team.id) %></td>
        <td><% if @completed[team] %>
                 Yes
            <% else %>
                 No
            <% end %>
       </td>
      </tr>
    <% end %>
  </tbody>
</table>
    
<div>
    <span>
        <a href="">View My Tickets</a>
        <a href="">Create A Ticket</a>
    </span>
</div>
